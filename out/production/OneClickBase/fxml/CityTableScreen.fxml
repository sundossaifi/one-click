<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="588.0" prefWidth="553.0" styleClass="Table-view" stylesheets="@../CSS/CarTableScreen.CSS" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CityTableController">
   <children>
      <TableView fx:id="cityTable" focusTraversable="false" layoutX="1.0" layoutY="53.0" prefHeight="535.0" prefWidth="552.0">
         <columns>
            <TableColumn fx:id="idCol" editable="false" prefWidth="92.0" resizable="false" sortable="false" text="ID" />
            <TableColumn fx:id="nameCol" editable="false" prefWidth="137.0" resizable="false" sortable="false" text="Name" />
            <TableColumn fx:id="Description" editable="false" prefWidth="322.0" resizable="false" sortable="false" text="description" />
         </columns>
      </TableView>
      <Button fx:id="add" focusTraversable="false" layoutX="224.0" layoutY="11.0" mnemonicParsing="false" onAction="#addcity" prefHeight="25.0" prefWidth="106.0" text="Button">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <TextField fx:id="searchField" layoutX="39.0" layoutY="10.0" prefHeight="33.0" prefWidth="177.0" promptText="Type here to search" />
      <Button fx:id="search" layoutX="6.0" layoutY="14.0" mnemonicParsing="false" onAction="#searchEvent" styleClass="mybutton2" stylesheets="@../CSS/adminscreen.CSS">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
